
.graticule:
	fill: none
	stroke: #777
	stroke-width: .5px
	stroke-opacity: .5


.country:
	-webkit-transition: fill 0.25s linear
	&:hover:
		fill: #4C4747
	stroke: #8F9092
	stroke-width: 1px
	stroke-opacity: 0.4


.entity:
	fill: #E3E8EC
  	-webkit-transition: r,stroke,stroke-width  0.25s linear
	&:hover:
		fill: #fff
	stroke-width: 0.7px
	stroke:#9F9F9F


.dialog:
	background-color:green

.legend:
	&.line:
		stroke: black
	&.text:
		color:#717171