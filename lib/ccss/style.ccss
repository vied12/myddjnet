body:
	margin  : 0
	padding : 0
	text-align:center

.Map:
	.graticule:
		fill             : none
		stroke           : #777
		stroke-width     : .5px
		stroke-opacity   : .5

	.country:
		-webkit-transition : fill 0.25s linear
		stroke             : #8F9092
		stroke-width       : 1px
		stroke-opacity     : 0.4
		&:hover:
			fill: #4C4747

	.city:
		font-size: 0.7em

	.entity:
		fill               : #2B2B2B
	  	-webkit-transition : r,stroke,stroke-width  0.25s linear
		stroke-width       : 2px
		stroke             : #9F9F9F
		&.person:
			stroke: #A8D8FF
		&.company:
			stroke: #F8BDBD
		&:hover:
			fill: #616161

	.dialog:
		background-color : green

	.legend:
		&.line:
			stroke : black
		&.text:
			font-size:1em
			color : #717171

.primary.Map:
	margin-top: 20px
	svg:
		border: 2px solid #444444
		background-color: #E7EAF7

.Panel:
	overflow:hidden
	min-height: 100px